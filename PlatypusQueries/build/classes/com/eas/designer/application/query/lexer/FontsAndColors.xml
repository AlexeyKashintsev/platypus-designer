<!DOCTYPE fontscolors PUBLIC
    "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN"
    "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor name="string" default="string"/>
    <fontcolor name="errors" default="error"/>
    <fontcolor name="identifier" default="identifier" foreColor="ff006633"/>
    <fontcolor name="keyword" default="keyword" foreColor="blue"/>
    <fontcolor name="multilinecomment" default="multilinecomment" foreColor="gray"/>
    <fontcolor name="linecomment" default="linecomment" foreColor="gray"/>
    <fontcolor name="special" default="identifier" foreColor="ff666600"/>
    <fontcolor name="number" default="number"/>
    <fontcolor name="operator" default="operator" foreColor="ff990000"/>
</fontscolors>
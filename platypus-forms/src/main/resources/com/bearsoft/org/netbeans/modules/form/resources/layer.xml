        <folder name="Modes">
            <folder name="navigator">
                <file name="PlatypusFormInspector.wstcref" url="windowmanager/inspector/PlatypusFormInspector.wstcref">
                    <attr name="position" intvalue="200"/>
                </file>
            </folder>
        </folder>

        <folder name="Components">
            <file name="PlatypusFormInspector.settings" url="components/PlatypusFormInspector.settings"/>
        </folder>

        <folder name="Groups">
            <file name="jsform.wsgrp" url="groups/form.wsgrp"/>
            <folder name="jsform">
                <file name="PlatypusFormInspector.wstcgrp" url="groups/form/PlatypusFormInspector.wstcgrp"/>
                <file name="properties.wstcgrp" url="groups/form/properties.wstcgrp"/>
            </folder>
        </folder>

    </folder>

    <folder name="OptionsDialog">
        <folder name="Advanced">
            <file name="FormEditor.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory"/>

                <attr name="displayName" bundlevalue="com.bearsoft.org.netbeans.modules.form.Bundle#Form_Editor"/>
                <attr name="toolTip" bundlevalue="com.bearsoft.org.netbeans.modules.form.Bundle#Form_Editor_Tooltip"/>
                <attr name="controller" newvalue="com.bearsoft.org.netbeans.modules.form.FormEditorPanelController"/>
                <attr name="keywords" bundlevalue="com.bearsoft.org.netbeans.modules.form.Bundle#KW_FormOptions"/>
                <attr name="keywordsCategory" stringvalue="Advanced/FormEditor"/>
            </file>
        </folder>
    </folder>

    <folder name="OptionsExport">
        <folder name="Advanced">
            <file name="Form">
                <attr name="include" stringvalue="config/FormDesignerPalette/.*|config/Services/formsettings[.]settings"/>
                <attr name="displayName" bundlevalue="com.bearsoft.org.netbeans.modules.form.Bundle#Form.Options.Export.displayName"/>
            </file>
        </folder>
